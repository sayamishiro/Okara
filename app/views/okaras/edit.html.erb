<%= form_for @okara do |f| %>
    <%= f.label :タイトル %>
    <%= f.text_field :title, :size => 140 %> 
    <br>
    <%= f.label :午前目的地１ %>
    <%= f.text_field :gozen1, :size => 140 %> 
    <br>
    <%= f.label :交通手段１ %>
     <%= f.select :access1, 
    [["北鉄バス", "北鉄バス"], 
    ["徒歩", "徒歩"], 
    ["電車", "電車"]], include_blank: "選択して下さい"  %> 
    <br>
    <%= f.label :午前目的地２ %>
    <%= f.text_field :gozen2, :size => 140 %> 
    <br>
    <%= f.label :交通手段２ %>
    <%= f.select :access2, 
    [["北鉄バス", "北鉄バス"], 
    ["徒歩", "徒歩"], 
    ["電車", "電車"]], include_blank: "選択して下さい"  %>
    <br>
    <%= f.label :午後目的地１ %>
    <%= f.text_field :gogo1, :size => 140 %> 
    <br>
    <%= f.label :交通手段３ %>
    <%= f.select :access3, 
    [["北鉄バス", "北鉄バス"], 
    ["徒歩", "徒歩"], 
    ["電車", "電車"]], include_blank: "選択して下さい"  %>
    <br>
    <%= f.label :午後目的地２ %>
    <%= f.text_field :gogo2, :size => 140 %> 
    <br>
    <%= f.label :交通手段４ %>
    <%= f.select :access4, 
    [["北鉄バス", "北鉄バス"], 
    ["徒歩", "徒歩"], 
    ["電車", "電車"]], include_blank: "選択して下さい"  %> 
    <br>
    <%= f.label :夜目的地 %>
    <%= f.text_field :yoru, :size => 140 %> 
    
    <%= f.submit "編集する" %>
  <% end %>